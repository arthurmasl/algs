{
  "env": {
    "browser": true,
    "es2020": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": 0,
    "implicit-arrow-linebreak": 0,
    "no-extra-parens": 0,
    "no-nested-ternary": 0,
    "no-confusing-arrow": 0
  }
}
